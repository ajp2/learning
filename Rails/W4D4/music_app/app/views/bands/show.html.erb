<h2><%= @band.name %></h2>

<form action="<%= edit_band_url(@band) %>" method="get">
  <input type="submit" value="Edit">
</form>

<form action="<%= band_url(@band) %>" method="post">
  <%= auth_token%>
  <input type="hidden" name="_method" value="delete">
  <input type="submit" value="Delete">
</form>